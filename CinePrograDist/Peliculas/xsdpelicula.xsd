<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"	xmlns:tns1="http://com.cine" targetNamespace="http://com.cine" elementFormDefault="qualified" attributeFormDefault="unqualified">	<xs:element name="pelicula"> <!-- Elemento principal-->		<xs:complexType>			<xs:sequence>				<xs:element name="idpelicula">					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:pattern value="^[0-9]{4}[A-Za-z]{4}$"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="nombre">					<xs:simpleType>						<xs:restriction base="xs:string">						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="genero">					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:enumeration value="comedia"/>							<xs:enumeration value="romance"/>							<xs:enumeration value="drama"/>							<xs:enumeration value="terror"/>							<xs:enumeration value="documental"/>							<xs:enumeration value="ficcion"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="fecha">					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:pattern value="^[0-9]{2}-[0-9]{2}-[0-9]{4}$"/>	<!-- día-mes-año -->						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="hora">	<!-- La hora nos había dicho que convenía manejarla con un patrón-->					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:pattern value="^[0-9]{2}:[0-9]{2}$"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="duracion">					<xs:simpleType>						<xs:restriction base="xs:int">							</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="idsala"> <!-- Es un criterio de consulta de las películas-->					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:minInclusive value="1"/>							<xs:maxInclusive value="3"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="sinopsis">				<xs:simpleType>						<xs:restriction base="xs:string">						<xs:maxLength value="200"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<!-- Conviene manejar al reparto como un elemento independiente y solo poner su id acá.-->				<xs:element name="protagonista">				<xs:simpleType>								<xs:restriction base="xs:string">						<xs:pattern value="^PRT[A-Za-z]{2}[0-9]{3}[A-Za-z]{4}$"/>					</xs:restriction>						</xs:simpleType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element>		<xs:element name="protagonista">	<!-- Esto no sé si va acá también. Podría ponerse en otro archivo y solo referenciarse su atributo que pertenece al elemento 'película'.-->				<xs:complexType>							<xs:sequence>							<xs:element name="idprota">								<xs:simpleType>									<xs:restriction base="xs:string">									<xs:pattern value="^PRT[A-Za-z]{2}[0-9]{3}[A-Za-z]{4}$"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="nombre">								<xs:simpleType>									<xs:restriction base="xs:string">									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="apellido">								<xs:simpleType>									<xs:restriction base="xs:string">									</xs:restriction>								</xs:simpleType>							</xs:element>						</xs:sequence>				</xs:complexType>	</xs:element>	<xs:element name="sala"> <!-- Esto no sé si va acá también. Podría ponerse en otro archivo y solo referenciarse su atributo que pertenece al elemento 'película'.-->		<xs:complexType>			<xs:sequence>				<xs:element name="idsala">					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:minInclusive value="1"/>							<xs:maxInclusive value="3"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="capacidad">					<xs:simpleType>						<xs:restriction base="xs:int">						</xs:restriction>					</xs:simpleType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>