<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"	xmlns:tns1="http://com.cine" targetNamespace="http://com.cine" elementFormDefault="qualified" attributeFormDefault="unqualified">	<xs:element name="servicio">		<xs:annotation>			<xs:documentation>Cine</xs:documentation>		</xs:annotation>		<xs:simpleType>			<xs:restriction base="xs:string">				<xs:enumeration value="altaPelicula"/>				<xs:enumeration value="bajaPelicula"/>				<xs:enumeration value="consultaPelicula"/>				<xs:enumeration value="modificarPelícula"/> <!--Servicio añadido--> 				<xs:enumeration value="comprarBoletos"/>				<xs:enumeration value="cancelarBoletos"/>	<!--Otro servicio añadido--> 			</xs:restriction>		</xs:simpleType>	</xs:element>		<xs:element name="pelicula">		<xs:complexType>			<xs:sequence>				<xs:element name="idpelicula">					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:pattern value="^[0-9]{4}[A-Za-z]{4}$"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="nombre">					<xs:simpleType>						<xs:restriction base="xs:string">						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="genero">					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:enumeration value="comedia"/>							<xs:enumeration value="romance"/>							<xs:enumeration value="drama"/>							<xs:enumeration value="terror"/>							<xs:enumeration value="documental"/>							<xs:enumeration value="ficcion"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="fecha">					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:pattern value="^[0-9]{2}-[0-9]{2}-[0-9]{4}$"/>	<!-- día-mes-año -->						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="hora">						<xs:simpleType>						<xs:restriction base="xs:int">	<!--¿La hora convendría manejarla como string y usar un regex?--> 						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="duracion">					<xs:simpleType>						<xs:restriction base="xs:int">							</xs:restriction>					</xs:simpleType>				</xs:element>   <!-- ¿El id de sala también debe ponerse aquí o existe forma de obtenerlo desde el elemento 'sala'?-->				<!--				<xs:element name="idsala">					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:minInclusive value="1"/>							<xs:maxInclusive value="3"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				-->				<xs:element name="sinopsis">				<xs:simpleType>						<xs:restriction base="xs:string">						<xs:maxLength value="200"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<!-- Conviene manejar al reparto como un elemento independiente y solo poner su id acá.-->				<xs:element name="protagonista">			<!--Como elemento simple:-->								<!--<xs:simpleType>						<xs:restriction base="xs:string">--> 						<!--						</xs:restriction>					</xs:simpleType 				-->			<!--Como elemento complejo (quizá es la forma más adecuada):-->					<xs:complexType>							<xs:sequence>							<xs:element name="idprota">								<xs:simpleType>									<xs:restriction base="xs:string">									<xs:pattern value="^PRT[A-Za-z]{2}[0-9]{3}[A-Za-z]{4}$"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="nombre">								<xs:simpleType>									<xs:restriction base="xs:string">									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="apellido">								<xs:simpleType>									<xs:restriction base="xs:string">									</xs:restriction>								</xs:simpleType>							</xs:element>						</xs:sequence>				</xs:complexType>				</xs:element>							</xs:sequence>		</xs:complexType>	</xs:element>		<xs:element name="funcion">		<xs:complexType>			<xs:sequence>				<xs:element name="idfuncion">					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:pattern value="^F0[1-3][A-Za-z]{3}[0-9]{2}$"/> <!--Id de sala a dos dígitos, tres primeras letras de la película y hora de la función a dos dígitos.--> 						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="horario">					<xs:simpleType>						<xs:restriction base="xs:int">						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="idpelicula">					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:pattern value="^[0-9]{4}[A-Za-z]{4}$"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="idsala">					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:minInclusive value="1"/>							<xs:maxInclusive value="3"/>						</xs:restriction>					</xs:simpleType>				</xs:element>							</xs:sequence>		</xs:complexType>	</xs:element>		<xs:element name="sala">		<xs:complexType>			<xs:sequence>				<xs:element name="idsala">					<xs:simpleType>						<xs:restriction base="xs:int">							<xs:minInclusive value="1"/>							<xs:maxInclusive value="3"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="capacidad">					<xs:simpleType>						<xs:restriction base="xs:int">						</xs:restriction>					</xs:simpleType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element>		<xs:element name="boleto">		<xs:complexType>			<xs:sequence>				<!--Agregado--> 				<xs:element name="idboleto">					<xs:simpleType>						<xs:restriction base="xs:string">								<xs:pattern value="^[A-Za-z]{3}0[0-9][0-9]{2}[A-Za-z]{4}$"/> <!-- Primeras tres letras del apellido,número de sala a dos dígitos, número de asiento a dos dígitos y cuatro letras aleatorias o según el nombre de la película.-->						</xs:restriction>					</xs:simpleType>					</xs:element>				<xs:element name="idfuncion">					<xs:simpleType>						<xs:restriction base="xs:string">								<xs:pattern value="^F0[1-3][A-Za-z]{3}[0-9]{2}$"/>						</xs:restriction>					</xs:simpleType>					</xs:element>				<xs:element name="nodeasiento">		<!--Añadido-->					<xs:simpleType>						<xs:restriction base="xs:int">								<xs:minInclusive value="1"/>						</xs:restriction>					</xs:simpleType>					</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element>		<xs:element name="cliente">		<xs:complexType>			<xs:sequence>				<xs:element name="idcliente">					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:pattern value="^CL[A-Za-z]{2}[A-Za-z]{2}[0-9]{4}$"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="nombre">					<xs:simpleType>						<xs:restriction base="xs:string">						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="apellido">					<xs:simpleType>						<xs:restriction base="xs:string">						</xs:restriction>					</xs:simpleType>				</xs:element>				<!-- La compra de boletos debe ser independiente y ahí llevar el id de cliente, función y número de boletos.-->				<xs:element name="boletosComprados">	<!--Añadido--> 						<xs:simpleType>						<xs:restriction base="xs:int">							<xs:maxInclusive value="4"/>						</xs:restriction>					</xs:simpleType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element>	</xs:schema>